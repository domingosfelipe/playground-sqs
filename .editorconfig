root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = tab
indent_size = 2

[*.java]
max_line_length = off

ij_java_class_count_to_use_import_on_demand = 99
ij_java_names_count_to_use_import_on_demand = 99

[*.xml]
max_line_length = off

[*.yml]
max_line_length = off

[*.md]
trim_trailing_whitespace = false
max_line_length = off